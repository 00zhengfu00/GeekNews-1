<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SToolbar">
        <attr name="navIcon" format="reference" />
        <attr name="menuIcon" format="reference" />
        <attr name="contentHeight" format="dimension" />
        <attr name="title" format="string" />
        <attr name="titleStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="titleSize" format="dimension" />
        <attr name="titleColor" format="color" />
    </declare-styleable>

    <declare-styleable name="NineGridImageLayout">
        <attr name="spacing" format="dimension" />
        <attr name="singleSize" format="dimension" />
    </declare-styleable>
</resources>